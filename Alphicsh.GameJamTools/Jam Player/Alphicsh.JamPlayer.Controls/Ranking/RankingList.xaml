<UserControl x:Class="Alphicsh.JamPlayer.Controls.Ranking.RankingList"
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
  xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
  xmlns:local="clr-namespace:Alphicsh.JamPlayer.Controls.Ranking"
  xmlns:rankingVm="clr-namespace:Alphicsh.JamPlayer.ViewModel.Ranking;assembly=Alphicsh.JamPlayer.ViewModel"
  mc:Ignorable="d" 
  d:DesignHeight="450" d:DesignWidth="400"
>
  <ListBox
    ItemsSource="{Binding}"
    HorizontalContentAlignment="Stretch"
    ScrollViewer.VerticalScrollBarVisibility="Visible"
  >
    <ListBox.Resources>
      <DataTemplate DataType="{x:Type rankingVm:RankingEntryViewModel}">
        <local:RankingListEntry DataContext="{Binding}"></local:RankingListEntry>
      </DataTemplate>
    </ListBox.Resources>
  </ListBox>
</UserControl>
